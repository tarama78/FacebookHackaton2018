{
"manifest_version": 2,
    "name": "Facebook",
    "description": "Facebook news",
    "version": "1.0",
	
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },

    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
